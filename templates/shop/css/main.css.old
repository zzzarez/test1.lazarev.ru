.radgrad
{
    position: relative;

    border:1px solid #b8bdad;

    border-radius: 0px 0px 10px 10px;
	-webkit-border-radius: 0px 0px 10px 10px;
	-moz-border-radius: 0px 0px 10px 10px;

/*	-webkit-box-shadow: #666 0px 2px 3px;
	-moz-box-shadow: #666 0px 2px 3px;
	box-shadow: #666 0px 2px 3px;
*/
	background: #f6fbe1;
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#ffffff), to(#f6fbe1));
	background: -webkit-linear-gradient(#ffffff, #f6fbe1);
	background: -moz-linear-gradient(#ffffff, #f6fbe1);
	background: -ms-linear-gradient(#ffffff, #f6fbe1);
	background: -o-linear-gradient(#ffffff, #f6fbe1);
	background: linear-gradient(#ffffff, #f6fbe1);
	-pie-background: linear-gradient(#ffffff, #f6fbe1);
	behavior: url(PIE.php);

}
.radgradwhite
{
    position: relative;
    border:1px solid #b8bdad;
    border-radius: 10px;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	-webkit-box-shadow: #666 1px 1px 3px;
	-moz-box-shadow: #666 1px 1px 3px;
	box-shadow: #666 1px 1px 3px;
	background: white;
	behavior: url(PIE.php);
}

